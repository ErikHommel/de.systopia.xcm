<?xml version="1.0"?>
<extension key="de.systopia.xcm" type="module">
  <file>xcm</file>
  <name>Extended Contact Matcher (XCM)</name>
  <description>CiviCRM contact matching extension.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.xcm</url>
    <url desc="Documentation">https://docs.civicrm.org/xcm/en/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.xcm/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-21</releaseDate>
  <version>1.11.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.35</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Xcm</namespace>
    <format>22.05.2</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
